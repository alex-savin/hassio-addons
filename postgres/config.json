{
    "name": "PostgreSQL DB",
    "version": "0.2.13",
    "slug": "postgres",
    "description": "An SQL database server",
    "arch": ["amd64", "i386", "armhf", "armv7", "aarch64"],
    "url": "https://github.com/alex-savin/hassio-addons/tree/master/postgres",
    "startup": "system",
    "webui": "http://[HOST]:[PORT:8099]",
    "ingress": true,
    "ingress_port": 8099,
    "panel_title": "PostgreSQL",
    "panel_icon": "mdi:database",
    "boot": "auto",
    "ports": {
	"8099/tcp": null,
	"5432/tcp": 5432
    },
    "ports_description": {
	"8081/tcp": "pgWeb",
	"5432/tcp": "PostgreSQL"
    },
    "docker_api": false,
    "hassio_api": true,
    "hassio_role": "default",
    "homeassistant_api": false,
    "host_network": false,
    "host_ipc": false,
    "host_dbus": false,
    "host_pid": false,
    "auto_uart": false,
    "privileged": [],
    "full_access": false,
    "apparmor": false,
    "audio": false,
    "gpio": false,
    "devicetree": false,
    "stdin": false,
    "legacy": false,
    "options": {
	"log_level": "info",
	"databases": ["homeassistant"],
	"roles": [
	    {
		"username": "hassio",
		"password": "hassio"
	    }
	],
	"privileges": [
	    {
		"username": "hassio",
		"database": "homeassistant",
		"grant": "ALL PRIVILEGES"
	    }
	],
	"delete": {
	    "databases": [],
	    "roles": []
	}
    },
    "schema": {
	"log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
	"databases": ["str"],
	"roles": [
	    {
		"username": "str",
		"password": "str"
	    }
	],
	"privileges": [
	    {
		"username": "str",
		"database": "str",
		"grant": "match(^(SELECT|INSERT|UPDATE|DELETE|RULE|ALL PRIVILEGES)$)"
	    }
	],
	"delete": {
	    "databases": ["str?"],
	    "roles": ["str?"]
	}
    },
    "timeout": 20,
    "environment": {
	"LOG_FORMAT": "{LEVEL}: {MESSAGE}"
    },
    "image": "smarthouseint/{arch}-addon-postgres"
}
